{
  "name": "@chatgpt-web/config",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Shared configuration files for ChatGPT Web monorepo",
  "main": "index.js",
  "scripts": {
    "build": "node -e \"const fs = require('fs'); fs.mkdirSync('dist', { recursive: true }); fs.writeFileSync('dist/.placeholder', ''); console.log('Configuration package build complete');\"",
    "dev": "echo 'Configuration package in development mode'",
    "lint": "echo 'No linting needed for config package'",
    "lint:fix": "echo 'No linting needed for config package'",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "echo 'No TypeScript compilation needed for config package'",
    "test": "echo 'No tests in config package'",
    "quality": "run-p format:check",
    "quality:fix": "run-s format",
    "clean": "echo 'Nothing to clean in config package'"
  },
  "exports": {
    "./eslint": "./eslint.config.js",
    "./prettier": "./prettier.config.js",
    "./typescript": "./tsconfig.base.json",
    "./tsup": "./tsup.config.ts",
    "./vitest": "./vitest.config.ts",
    "./husky": "./husky.config.js",
    "./vscode/settings": "./vscode-settings.json",
    "./vscode/extensions": "./vscode-extensions.json",
    "./vscode/launch": "./vscode-launch.json",
    "./vscode/workspace": "./chatgpt-web.code-workspace"
  },
  "files": [
    "*.js",
    "*.json",
    "*.ts",
    "*.code-workspace",
    "vscode-*.json"
  ],
  "peerDependencies": {
    "@antfu/eslint-config": "^6.7.3",
    "eslint": "^9.39.2",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "tsup": "^8.5.1",
    "vitest": "^4.0.16",
    "husky": "^9.1.7",
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "lint-staged": "^16.2.7"
  }
}
