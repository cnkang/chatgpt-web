{
  "name": "@chatgpt-web/web",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "description": "ChatGPT Web Frontend Application",
  "scripts": {
    "dev": "vite --port 1002",
    "dev:debug": "vite --port 1002 --debug",
    "dev:host": "vite --port 1002 --host",
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "build:analyze": "ANALYZE=true vite build",
    "preview": "vite preview",
    "preview:host": "vite preview --host",
    "type-check": "vue-tsc --noEmit",
    "type-check:watch": "vue-tsc --noEmit --watch",
    "lint": "eslint . --fix --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "quality": "run-p type-check lint format:check",
    "quality:fix": "run-p type-check lint:fix format",
    "clean": "rimraf dist node_modules/.vite"
  },
  "dependencies": {
    "@chatgpt-web/shared": "workspace:*",
    "@iconify/vue": "^5.0.0",
    "@vscode/markdown-it-katex": "^1.0.0",
    "@vueuse/core": "^14.1.0",
    "crypto-js": "^4.2.0",
    "highlight.js": "^11.10.0",
    "html-to-image": "^1.11.11",
    "katex": "^0.16.11",
    "markdown-it": "^14.1.0",
    "markdown-it-link-attributes": "^4.0.1",
    "naive-ui": "^2.43.2",
    "pinia": "^3.0.0",
    "vue": "^3.5.26",
    "vue-i18n": "^11.0.0",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@chatgpt-web/config": "workspace:*",
    "@types/crypto-js": "^4.2.2",
    "@types/markdown-it": "^14.1.2",
    "@types/markdown-it-link-attributes": "^3.0.5",
    "@types/node": "^25.0.9",
    "@vitejs/plugin-vue": "^6.0.3",
    "autoprefixer": "^10.4.0",
    "less": "^4.2.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^4.1.18",
    "vite": "^7.3.1",
    "vite-plugin-pwa": "^1.2.0",
    "vue-tsc": "^3.2.2"
  }
}
