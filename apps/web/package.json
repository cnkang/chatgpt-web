{
  "name": "@chatgpt-web/web",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "description": "ChatGPT Web Frontend Application",
  "scripts": {
    "dev": "vite --port 1002",
    "dev:debug": "vite --port 1002 --debug",
    "dev:host": "vite --port 1002 --host",
    "build": "pnpm run type-check && pnpm run build-only",
    "build-only": "vite build",
    "build:analyze": "ANALYZE=true vite build",
    "preview": "vite preview",
    "preview:host": "vite preview --host",
    "type-check": "vue-tsc --noEmit",
    "type-check:watch": "vue-tsc --noEmit --watch",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "quality": "pnpm run type-check && pnpm run lint && pnpm run format:check",
    "quality:fix": "pnpm run type-check && pnpm run lint:fix && pnpm run format",
    "clean": "node -e \"const fs=require('node:fs'); fs.rmSync('dist',{recursive:true,force:true}); fs.rmSync('node_modules/.vite',{recursive:true,force:true});\"",
    "test": "echo 'No tests in web package'",
    "test:integration": "echo 'No integration tests in web package'",
    "test:lighthouse": "node -e \"const fs=require('fs'); fs.mkdirSync('lighthouse-results',{recursive:true}); fs.writeFileSync('lighthouse-results/summary.json', JSON.stringify({status:'skipped', reason:'Lighthouse tests not implemented in repo'}, null, 2)); console.log('Lighthouse tests skipped: placeholder results generated');\""
  },
  "dependencies": {
    "@chatgpt-web/shared": "workspace:*",
    "@iconify/vue": "^5.0.0",
    "@vueuse/core": "^14.2.1",
    "html-to-image": "^1.11.13",
    "katex": "^0.16.28",
    "naive-ui": "^2.43.2",
    "pinia": "^3.0.4",
    "vue": "^3.5.28",
    "vue-i18n": "^11.2.8",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.2.0",
    "@types/node": "^25.3.0",
    "@vitejs/plugin-vue": "^6.0.4",
    "autoprefixer": "^10.4.24",
    "less": "^4.5.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.2.0",
    "vite": "^7.3.1",
    "vue-tsc": "^3.2.5"
  }
}
