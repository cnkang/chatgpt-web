{
  "name": "chatgpt-web-service",
  "type": "module",
  "version": "1.0.0",
  "private": false,
  "description": "ChatGPT Web Service",
  "author": "Kang Liu",
  "contributors": [
    "ChenZhaoYu <chenzhaoyu1994@gmail.com>"
  ],
  "keywords": [
    "chatgpt-web",
    "chatgpt",
    "chatbot",
    "express"
  ],
  "engines": {
    "node": ">=24.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "start": "esno ./src/index.ts",
    "dev": "esno watch ./src/index.ts",
    "prod": "node ./build/index.js",
    "build": "pnpm clean && tsup",
    "clean": "rimraf build",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix --max-warnings 0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "quality": "run-p type-check lint format:check",
    "quality:fix": "run-s lint:fix format type-check",
    "validate-cleanup": "esno ./src/validation/run-cleanup-validation.ts",
    "common:cleanup": "rimraf node_modules && rimraf pnpm-lock.yaml"
  },
  "dependencies": {
    "@types/connect-redis": "^0.0.23",
    "@types/express-session": "^1.18.2",
    "@types/jsdom": "^27.0.0",
    "axios": "^1.13.2",
    "chatgpt": "^5.2.5",
    "connect-redis": "^9.0.0",
    "dompurify": "^3.3.1",
    "dotenv": "^17.2.3",
    "esno": "^4.8.0",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "https-proxy-agent": "^7.0.6",
    "isomorphic-fetch": "^3.0.0",
    "jsdom": "^27.4.0",
    "node-fetch": "^3.3.2",
    "openai": "^6.15.0",
    "redis": "^5.10.0",
    "socks-proxy-agent": "^8.0.5",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.3",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "@vitest/ui": "^4.0.16",
    "eslint": "^9.39.2",
    "fast-check": "^4.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
