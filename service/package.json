{
  "name": "chatgpt-web-service",
  "type": "module",
  "version": "1.0.0",
  "private": false,
  "description": "ChatGPT Web Service",
  "author": "Kang Liu",
  "contributors": [
    "ChenZhaoYu <chenzhaoyu1994@gmail.com>"
  ],
  "keywords": [
    "chatgpt-web",
    "chatgpt",
    "chatbot",
    "express"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "esno ./src/index.ts",
    "dev": "esno watch ./src/index.ts",
    "prod": "node ./build/index.js",
    "build": "pnpm clean && tsup",
    "clean": "rimraf build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "common:cleanup": "rimraf node_modules && rimraf pnpm-lock.yaml"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "chatgpt": "^5.2.5",
    "dotenv": "^17.2.3",
    "esno": "^4.8.0",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "https-proxy-agent": "^7.0.6",
    "isomorphic-fetch": "^3.0.0",
    "node-fetch": "^3.3.2",
    "socks-proxy-agent": "^8.0.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.3",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "eslint": "^9.39.2",
    "rimraf": "^6.1.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
