{
  "name": "chatgpt-web",
  "version": "3.0.1",
  "private": false,
  "packageManager": "pnpm@10.29.2+sha512.bef43fa759d91fd2da4b319a5a0d13ef7a45bb985a3d7342058470f9d2051a3ba8674e629672654686ef9443ad13a82da2beb9eeb3e0221c87b8154fff9d74b8",
  "description": "ChatGPT Web",
  "author": "Kang",
  "contributors": [
    "ChenZhaoYu"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cnkang/chatgpt-web.git"
  },
  "bugs": {
    "url": "https://github.com/cnkang/chatgpt-web/issues"
  },
  "homepage": "https://github.com/cnkang/chatgpt-web#readme",
  "engines": {
    "node": "24.x"
  },
  "keywords": [
    "chatgpt-web",
    "chatgpt",
    "chatbot",
    "vue"
  ],
  "scripts": {
    "dev": "vite",
    "build": "pnpm run type-check && pnpm run build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "pnpm run type-check:web && pnpm run type-check:service",
    "type-check:web": "vue-tsc --noEmit",
    "type-check:service": "pnpm --dir service run type-check",
    "lint": "oxlint --vue-plugin --import-plugin src service/src",
    "lint:strict": "oxlint --vue-plugin --import-plugin -D all src service/src",
    "lint:ox": "pnpm run lint",
    "lint:ox:strict": "pnpm run lint:strict",
    "secrets:scan": "gitleaks dir . --no-banner --redact",
    "secrets:scan:staged": "gitleaks git --staged --no-banner --redact",
    "secrets:scan:history": "gitleaks git --no-banner --redact --log-opts='--all'",
    "bootstrap": "pnpm install && pnpm run common:prepare",
    "common:cleanup": "node -e \"import('node:fs').then(({ rmSync }) => { rmSync('node_modules', { recursive: true, force: true }); rmSync('pnpm-lock.yaml', { force: true }); })\"",
    "common:prepare": "husky install"
  },
  "dependencies": {
    "@vscode/markdown-it-katex": "^1.1.2",
    "highlight.js": "^11.11.1",
    "html-to-image": "^1.11.13",
    "katex": "^0.16.28",
    "markdown-it": "^14.1.1",
    "mermaid": "^11.10.0",
    "mermaid-it-markdown": "^1.0.13",
    "naive-ui": "^2.43.2",
    "pinia": "^3.0.4",
    "vue": "^3.5.28",
    "vue-i18n": "^11.2.8",
    "vue-router": "^5.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@iconify/vue": "^5.0.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/markdown-it": "^14.1.2",
    "@types/markdown-it-link-attributes": "^3.0.5",
    "@types/node": "^24.10.13",
    "@vitejs/plugin-vue": "^6.0.4",
    "husky": "^9.1.7",
    "less": "^4.5.1",
    "lint-staged": "^16.2.7",
    "markdown-it-link-attributes": "^4.0.1",
    "oxlint": "^1.46.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-pwa": "^1.2.0",
    "vue-tsc": "^3.2.4",
    "workbox-build": "^7.4.0",
    "workbox-window": "^7.4.0"
  },
  "lint-staged": {
    "*.{js,cjs,mjs,ts,tsx,vue}": [
      "oxlint --vue-plugin --import-plugin"
    ]
  }
}
