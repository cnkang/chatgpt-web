{
  "name": "chatgpt-web",
  "version": "2.11.1",
  "private": false,
  "packageManager": "pnpm@10.24.0+sha512.01ff8ae71b4419903b65c60fb2dc9d34cf8bb6e06d03bde112ef38f7a34d6904c424ba66bea5cdcf12890230bf39f9580473140ed9c946fef328b6e5238a345a",
  "description": "ChatGPT Web",
  "author": "ChenZhaoYu <chenzhaoyu1994@gmail.com>",
  "keywords": [
    "chatgpt-web",
    "chatgpt",
    "chatbot",
    "vue"
  ],
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "oxlint --vue-plugin --import-plugin src service/src",
    "lint:strict": "oxlint --vue-plugin --import-plugin -D all src service/src",
    "lint:ox": "pnpm run lint",
    "lint:ox:strict": "pnpm run lint:strict",
    "secrets:scan": "gitleaks dir . --no-banner --redact",
    "secrets:scan:staged": "gitleaks git --staged --no-banner --redact",
    "secrets:scan:history": "gitleaks git --no-banner --redact --log-opts='--all'",
    "bootstrap": "pnpm install && pnpm run common:prepare",
    "common:cleanup": "rimraf node_modules && rimraf pnpm-lock.yaml",
    "common:prepare": "husky install"
  },
  "dependencies": {
    "@vscode/markdown-it-katex": "^1.1.2",
    "@vueuse/core": "^14.2.0",
    "highlight.js": "^11.11.1",
    "html-to-image": "^1.11.13",
    "katex": "^0.16.28",
    "markdown-it": "^14.1.0",
    "mermaid": "^10.9.5",
    "mermaid-it-markdown": "^1.0.13",
    "naive-ui": "^2.43.2",
    "pinia": "^3.0.4",
    "vue": "^3.5.27",
    "vue-i18n": "^11.2.8",
    "vue-router": "^5.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@iconify/vue": "^5.0.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/markdown-it": "^14.1.2",
    "@types/markdown-it-link-attributes": "^3.0.5",
    "@types/node": "^25.2.2",
    "@vitejs/plugin-vue": "^6.0.4",
    "autoprefixer": "^10.4.24",
    "husky": "^9.1.7",
    "less": "^4.5.1",
    "lint-staged": "^16.2.7",
    "markdown-it-link-attributes": "^4.0.1",
    "npm-run-all": "^4.1.5",
    "oxlint": "^1.43.0",
    "postcss": "^8.5.6",
    "rimraf": "^6.1.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-pwa": "^1.2.0",
    "vue-tsc": "^3.2.4",
    "workbox-build": "^7.4.0",
    "workbox-window": "^7.4.0"
  },
  "lint-staged": {
    "*.{js,cjs,mjs,ts,tsx,vue}": [
      "oxlint --vue-plugin --import-plugin"
    ]
  }
}
